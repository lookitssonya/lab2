#include <iostream>
#include <fstream>
using namespace std;

int main() {
    ifstream in("f.txt"); //открываем первый файл для чтения 
    const int n = 150;
    
    double components[n]; // создаем массив для хранения компонентов файла
    for (int i = 0; i < n; i++) { // считываем компоненты файла в массив
        in >> components[i];
    }
    
    in.close();
    // Считаем сумму компонентов файла
    double sum = 0;
    for (int i = 0; i < n; i++) {
        sum += components[i];
    }

    ofstream out("f1.txt");
    out << "Сумма компонентов файла: " << sum << endl;  // выводим сумму компонентов файла в выходной файл
    out.close();
    return 0;
}
